    "@types/luxon": "^3.3.7",
    "@types/node": "^18.18.0",
    "browser-sync": "^3.0.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
